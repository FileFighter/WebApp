{
  "tsconfig": "tsconfig.json",
  "includes": "src",
  "exclude": [
    "./**/*.stories.*",
    "./**/__tests__/*",
    "sonar-scanner.js",
    "serviceWorker.ts",
    "react-app-env.d.ts",
    "setupTests"
  ],
  "out": "docs",
  "readme": "README.md",
  "entryPointStrategy": "Expand",
  "entryPoints": [
    "./src"
  ],
  "categorizeByGroup": true,
  "theme": "hierarchy",
  "customTitle": "FileFighter WebApp - Documentation",
  "customDescription": "API-Documentation for FileFighter's Web App",
  "favicon": "docs_assets/favicon_blue.ico",
  "footerTypedocVersion": true,
  "footerDate": true
}